

<ul class="thumbnails" ng-controller="PhotoController"  when-scrolled="loadMore()" style="overflow: auto; height: 1000px" >

<h2>{{photos.length}} photos found</h2>

  <li ng-repeat="photo in photos" class="span3">
    <div class="btn-group">
      <a href="#" class="dropdown-toggle thumbnail" data-toggle="dropdown"> 
        <img ng-src="{{photo.src}}">
      </a>
      <ul class="dropdown-menu">
        <!-- dropdown menu links -->
        <a href="share">Share this photo</a>
        <a href="share?date={{photo.taken}}">Share all photos this day</a>
      </ul>
    </div>
   </li>
</ul>

<script type="text/javascript" src="/js/photo.js"></script>

<script src="/js/jquery.masonry.min.js"></script>

